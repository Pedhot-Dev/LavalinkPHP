{
    "name": "pedhot-dev/lavalink-php",
    "description": "A powerful, asynchronous PHP library for Lavalink, ported from Python's Wavelink.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pedhot-Dev",
            "email": "agusetyawan230805@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "evenement/evenement": "*",
        "pedhot-dev/nepotismfree-di": "^1.1",
        "phpdocumentor/reflection-docblock": "*",
        "ratchet/pawl": "*",
        "react/http": "*",
        "symfony/property-access": "*",
        "symfony/property-info": "*",
        "symfony/serializer": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^2.0",
        "phpunit/phpunit": "^10.0 <10.5.63",
        "brianium/paratest": "^7.0 <7.4",
        "team-reflex/discord-php": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "PedhotDev\\LavalinkPHP\\": "src/"
        },
        "exclude-from-classmap": [
            "examples/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "archive": {
        "exclude": [
            "/examples"
        ]
    },
    "config": {
        "audit": {
            "block-insecure": false
        },
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "platform": {
            "php": "8.1.0"
        }
    }
}